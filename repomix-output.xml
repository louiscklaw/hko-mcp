import { ImakeRhrreadResponse } from '../../src/types/ImakeRhrreadResponse.js';
import { ImakeFlwResponse } from '../../src/types/ImakeFlwResponse.js';

describe('makeFlwRequest.test', () => {
  beforeEach(() => {
    global.fetch = vi.fn();
